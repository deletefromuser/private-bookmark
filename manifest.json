{
  "manifest_version": 3,
  "name": "Private Bookmark",
  "version": "1.0",
  "description": "Add/edit/delete/view private bookmarks. Password-protected view.",
  "permissions": ["storage", "tabs", "bookmarks"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Private Bookmark"
  },
  "background": {
    "service_worker": "background.js"
  }
  ,
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}
